{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
	  "allowJs": true,
	  "checkJs": false,
	  "moduleResolution": "bundler",
	  "baseUrl": ".",
	  "paths": {
		"$lib/*": ["src/lib/*"],
		"$routes/*": ["src/routes/*"]
	  }
	},
	"include": [
	  "src/**/*",
	  "src/**/*.svelte"
	]
  }
  